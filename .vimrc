filetype on
syntax on
set guifont=Menlo\ Regular:h18
set lines=35 columns=150
set colorcolumn=90
set number
let mapleader=" "
map <leader>s :source ~/.vimrc<CR>
set hidden
set history=100
filetype indent on
set nowrap
set tabstop=4
set shiftwidth=4
set expandtab
set smartindent
set autoindent
set hlsearch
nnoremap <Leader><Leader> :e#<CR>
set showmatch
